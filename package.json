{
  "name": "aabbdd",
  "version": "1.0.0",
  "description": "A class for convenient, method rich, Axis Aligned Bounding Boxes in 2D.",
  "main": "dist/index.js",
  "scripts": {
    "start": "cowboy-hat",
    "test": "ava --reporter=nyan",
    "watch": "./node_modules/.bin/ava --watch",
    "coverage": "./node_modules/.bin/nyc  --reporter=lcov ./node_modules/.bin/ava --watch",
    "build": "babel src --out-dir dist --source-maps",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/limeandcoconut/aabbdd.git"
  },
  "keywords": [
    "aabb",
    "2d",
    "bounding",
    "box",
    "axis",
    "aligned",
    "rectangle",
    "collision",
    "geometry",
    "square"
  ],
  "author": "Jacob Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/limeandcoconut/aabbdd/issues"
  },
  "homepage": "https://github.com/limeandcoconut/aabbdd#readme",
  "dependencies": {
    "cowboy-hat": "^2.0.0-alpha.10",
    "friendly-vectors": "^0.1.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "nyc": "^11.3.0",
    "tap-nyan": "^1.1.0"
  }
}
